div.body div.count
  display inline-block
  float right
  position relative
  font-size 35px

div.body .dropdown-container
  position relative
  width 300px
  padding 5px
  margin 5px
  background #A2A6AB
  color #fff
  outline none
  cursor pointer
  font-weight bold
  font-size 13
  border 2px solid black
  display inline-block

div.body .dropdown-container ul
  overflow auto
  height 200px 

div.body .dropdown-container:after 
  content ""
  width 0
  height 0
  position absolute
  right 16px
  top 50%
  margin-top -6px
  border-width 6px 0 6px 6px
  border-style solid
  border-color transparent #ffffff   

div.body .dropdown-container .dropdown 
  position absolute
  top 56%
  left 0
  right 0
  background #fff
  font-weight normal
  opacity 0
  pointer-events none

div.body .dropdown-container .dropdown li a 
  display block
  text-decoration none
  color #9e9e9e
  padding 10px 20px

div.body .dropdown-container span
  color #000000

div.body .dropdown-container .dropdown li:hover a 
  background #f3f8f8

div.body .dropdown-container.active .dropdown 
  opacity 1
  pointer-events auto
  z-index 500
 
div.body .dropdown-container.active after 
  border-color #9bc7de transparent
  border-width 6px 6px 0 6px 
  margin-top -3px

div.body .dropdown-container.active 
  background #9bc7de
  background linear-gradient(right, #9bc7de 0%, #9bc7de 78%, #ffffff 78%, #ffffff 100%)

.hidden
  display none
  
#hidden
  display none

.taintTrace
  width 500px
  
span.vulnerable
  background-color #F75C5C
  
div.code
  background-color #B6B6B6
  font-family monospace
  font-size 12px
  margin 24px
  overflow scroll
  white-space nowrap
  
.open
  overflow scroll
  
div.permlink
  display inline-block
  
#trace
  overflow hidden
  height 32px